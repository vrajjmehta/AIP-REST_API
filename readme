[
    {
        "reward_name": "chocolate"
    },
    {
        "reward_name": "coffee"
    },
    {
        "reward_name": "cupcake"
    },
    {
        "reward_name": "mint"
    },
    {
        "reward_name": "pizza"
    }
]
